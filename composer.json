{
    "name": "marceloxp/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
	"version" : "0.0.13",
	"minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
		"digitalnature/php-ref": "dev-master",
		"esemve/hook": "^0.0.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2@dev",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
		"files": [
			"app/Http/Umstudio/Helpers/helpers.php",
			"app/Http/Umstudio/Helpers/admin.php",
			"app/Http/Umstudio/Helpers/cached.php",
			"app/Http/Umstudio/Helpers/lang.php",
			"app/Http/Umstudio/Helpers/developer.php",
			"app/Http/Umstudio/Helpers/string.php",
			"app/Http/Umstudio/Helpers/filesystem.php"
		],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
